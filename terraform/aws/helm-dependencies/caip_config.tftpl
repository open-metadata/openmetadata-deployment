config:
  validation:
    # Maximum number of characters accepted in a user message.
%{ if caip.validation_max_user_message_chars != null ~}
    maxUserMessageChars: ${caip.validation_max_user_message_chars}
%{ endif ~}
  # Large language model provider configuration.
  llmProvider:
    # Type identifier of the configured LLM provider (e.g., bedrock, openai, anthropic, google, ollama).
%{ if caip.llm_type != null ~}
    type: "${caip.llm_type}"
%{ endif ~}
    # Provider-specific model identifier.
%{ if caip.llm_model != null ~}
    model: "${caip.llm_model}"
%{ endif ~}
    # Maximum reasoning iterations before forcing termination.
%{ if caip.llm_max_iteration != null ~}
    maxIteration: ${caip.llm_max_iteration}
%{ endif ~}
    # Maximum iterations when using dynamic cutoffs.
%{ if caip.llm_dynamic_max_iterations != null ~}
    dynamicMaxIterations: ${caip.llm_dynamic_max_iterations}
%{ endif ~}
    # OpenAI-compatible provider options.
    openAI:
      # API key used when OpenAI is selected.
%{ if caip.open_ai_api_key != null ~}
      apiKey: "${caip.open_ai_api_key}"
%{ endif ~}
      # Base URL for OpenAI API requests.
%{ if caip.open_ai_base_url != null ~}
      baseUrl: "${caip.open_ai_base_url}"
%{ endif ~}
      # Path for chat completions endpoint.
%{ if caip.open_ai_chat_completions_path != null ~}
      chatCompletionsPath: "${caip.open_ai_chat_completions_path}"
%{ endif ~}
      # Path for legacy completions/responses endpoint.
%{ if caip.open_ai_responses_api_path != null ~}
      responsesAPIPath: "${caip.open_ai_responses_api_path}"
%{ endif ~}
      # Path for embeddings endpoint.
%{ if caip.open_ai_embeddings_path != null ~}
      embeddingsPath: "${caip.open_ai_embeddings_path}"
%{ endif ~}
      # Path for content moderation endpoint.
%{ if caip.open_ai_moderations_path != null ~}
      moderationsPath: "${caip.open_ai_moderations_path}"
%{ endif ~}
      # Azure OpenAI-specific settings.
      azureOpenAI:
        # When true, toggle Azure OpenAI integration logic.
%{ if caip.open_ai_azure_open_ai_enabled != null ~}
        enabled: ${caip.open_ai_azure_open_ai_enabled}
%{ endif ~}
        # Azure OpenAI API version string.
%{ if caip.open_ai_azure_open_ai_api_version != null ~}
        apiVersion: "${caip.open_ai_azure_open_ai_api_version}"
%{ endif ~}
        # Azure deployment name that serves the model.
%{ if caip.open_ai_azure_open_ai_deployment_name != null ~}
        deploymentName: "${caip.open_ai_azure_open_ai_deployment_name}"
%{ endif ~}
        # Azure resource group name hosting the deployment.
%{ if caip.open_ai_azure_open_ai_resource_name != null ~}
        resourceName: "${caip.open_ai_azure_open_ai_resource_name}"
%{ endif ~}
    # Anthropic provider settings.
    anthropic:
      # API key used for Anthropic requests.
%{ if caip.anthropic_api_key != null ~}
      apiKey: "${caip.anthropic_api_key}"
%{ endif ~}
      # Base URL for Anthropic API calls.
%{ if caip.anthropic_base_url != null ~}
      baseUrl: "${caip.anthropic_base_url}"
%{ endif ~}
      # API version string for Anthropic requests.
%{ if caip.anthropic_api_version != null ~}
      apiVersion: "${caip.anthropic_api_version}"
%{ endif ~}
    # Google Generative AI provider settings.
    google:
      # API key used for Google Generative AI.
%{ if caip.google_api_key != null ~}
      apiKey: "${caip.google_api_key}"
%{ endif ~}
      # Base URL for Google Generative AI API calls.
%{ if caip.google_base_url != null ~}
      baseUrl: "${caip.google_base_url}"
%{ endif ~}
    # Amazon Bedrock provider settings.
    bedrock:
      # AWS access key ID when using static credentials.
%{ if caip.bedrock_aws_access_key_id != null ~}
      awsAccessKeyId: "${caip.bedrock_aws_access_key_id}"
%{ endif ~}
      # AWS secret access key matching the above ID.
%{ if caip.bedrock_aws_secret_access_key != null ~}
      awsSecretAccessKey: "${caip.bedrock_aws_secret_access_key}"
%{ endif ~}
      # AWS region hosting the Bedrock endpoint.
%{ if caip.bedrock_aws_region != null ~}
      awsRegion: "${caip.bedrock_aws_region}"
%{ endif ~}
      # Optional AWS session token for temporary credentials.
%{ if caip.bedrock_aws_session_token != null ~}
      awsSessionToken: "${caip.bedrock_aws_session_token}"
%{ endif ~}
      # Service-specific base URL override for Bedrock.
%{ if caip.bedrock_base_url != null ~}
      baseUrl: "${caip.bedrock_base_url}"
%{ endif ~}
      # Enables verbose logging for Bedrock requests when true.
%{ if caip.bedrock_enable_logging != null ~}
      enableLogging: ${caip.bedrock_enable_logging}
%{ endif ~}
      # Request timeout in milliseconds for Bedrock calls.
%{ if caip.bedrock_request_timeout_millis != null ~}
      requestTimeoutMillis: ${caip.bedrock_request_timeout_millis}
%{ endif ~}
      # Connection timeout in milliseconds for Bedrock calls.
%{ if caip.bedrock_connect_timeout_millis != null ~}
      connectTimeoutMillis: ${caip.bedrock_connect_timeout_millis}
%{ endif ~}
      # Socket timeout in milliseconds for Bedrock calls.
%{ if caip.bedrock_socket_timeout_millis != null ~}
      socketTimeoutMillis: ${caip.bedrock_socket_timeout_millis}
%{ endif ~}
    # Ollama provider settings.
    ollama:
      # Base URL of the Ollama server.
%{ if caip.ollama_base_url != null ~}
      baseUrl: "${caip.ollama_base_url}"
%{ endif ~}
